Cell[TextData[StyleBox["The following code generates the walking pattern by \
doing a 1 D Random Walk with 400 steps using randList1. If we get a value < \
0.5, move left by one step; otherwise, move right by one step. From the graph \
we got below, there is an obvious periodic pattern that repeats around step \
126.",
 FontSize->18]], "Text",
 CellChangeTimes->{{3.734625609211754*^9, 3.734625706878723*^9}, {
  3.7346262617828207`*^9, 3.73462626197059*^9}, {3.734719587981386*^9, 
  3.734719750389868*^9}},ExpressionUUID->"1fcf9b45-6bfa-4e0a-a09a-\
3cb27cb71e68"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"steps", "[", "p_", "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"p", "<", "0.5"}], ",", 
     RowBox[{"-", "1"}], ",", "1"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"stepsTrac", " ", "=", 
   RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"i", "=", "1"}], ",", 
   RowBox[{"i", "\[LessEqual]", "400"}], ",", 
   RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"AppendTo", "[", 
     RowBox[{"stepsTrac", ",", 
      RowBox[{"steps", "[", 
       RowBox[{"randList1", "[", 
        RowBox[{"[", "i", "]"}], "]"}], "]"}]}], "]"}], ";"}]}], 
  "\[IndentingNewLine]", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"xValues", "=", 
   RowBox[{"FoldList", "[", 
    RowBox[{"Plus", ",", "0", ",", "stepsTrac"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"ListLinePlot", "[", 
  RowBox[{"xValues", ",", 
   RowBox[{"PlotLabel", "\[Rule]", 
    RowBox[{"Style", "[", 
     RowBox[{"\"\<Random walk step tracking\>\"", ",", 
      RowBox[{"FontSize", "\[Rule]", "12"}]}], "]"}]}]}], 
  "]"}], "\[IndentingNewLine]"}], "Input",
